<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net8.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Desktop.Robot" Version="1.5.0" />
      <PackageReference Include="HttpMultipartParser" Version="8.3.0" />
      <PackageReference Include="LightHTTP" Version="1.0.2" />
      <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
      <PackageReference Include="ScreenCapturer" Version="3.0.1" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="index.html" />
      <EmbeddedResource Include="index.html" />
    </ItemGroup>

</Project>
